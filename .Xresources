! .Xdefaults.normal

!Transparency
URxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*.shading: 15

! Colours
!URxvt*foreground:       #BBBBBB
URxvt*foreground:       #508695
!URxvt*background:       #191919

!URxvt*colorBD:          #EEEEEF

! Black
URxvt*color0:           #404040
!URxvt*color8:           #252525

! Red
URxvt*color1:           #C12121
URxvt*color9:           #E50E0E

! Green
URxvt*color2:           #75BD00
URxvt*color10:          #89b83f

! Yellow
URxvt*color3:           #EDD76A
URxvt*color11:          #efef60

! Blue
URxvt*color4:           #4F6FAE
URxvt*color12:          #3F6FD0

! Magenta
URxvt*color5:           #706c9a
URxvt*color13:          #826ab1

! Cyan
URxvt*color6:           #508695
URxvt*color14:          #a1cdcd

! White
URxvt*color7:           #BBBBBB
URxvt*color15:          #EEEEEF

! Settings
URxvt*termName:             rxvt-256color
URxvt*loginShell:           false
URxvt*scrollBar:            false
URxvt*saveLines:            10000
! URxvt*font:                 xft:Liberation Mono:size=9
URxvt*font:                 xft:Literation Mono Powerline:size=9

URxvt*fading:               40

URxvt*geometry:             120x60
URxvt*internalBorder:       0
!URxvt*perl-ext-common:      default,matcher
URxvt*colorUL:              white
URxvt*urlLauncher:          firefox
URxvt*matcher.button:       1
URxvt*cursorUnderline:      false
! URxvt*boldFont:             xft:Liberation Mono:bold:size=9
URxvt*boldFont:             xft:Literation Mono Powerline:bold:size=9
URxvt*scrollTtyOutput:      false
URxvt*scrollTtyKeypress:    true
URxvt*scrollKey:            true
URxvt*scrollWithBuffer:     false
URxvt*letterSpace:          -1
Xft.dpi:                   96
